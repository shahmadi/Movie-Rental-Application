<% include partials/header %>

   
        
       
      <% for(var i=req.length-1;i>=0;i--){ %>  
      <div style="margin-left: 30%"> 
             <% var r=req[i]; %>
            <p>Name: <h1><%= r.name %> </h1></p>
            <p>Phone: <%= r.phone %></p>
            <p>Email <%= r.email %></p>
            <p>Message <%= r.message %></p>
            <p>Time <%= r.time %></p>
            <p>Date <%= r.date %></p>
            
            <p>Date Requested: <%=r.datereq %></p>
             <form action="/req/<%= r._id %>?_method=DELETE" method="POST" align="center" style="margin-top: 10px">
            <div class="form-group" style="width: 20%; margin:auto auto;">
             <button class="btn btn-danger" style="margin-bottom:20px">DELETE</button>
             </div>
        </form> 
        </div>
            <hr>
            <% } %>
     
        
     
</body>
</html>